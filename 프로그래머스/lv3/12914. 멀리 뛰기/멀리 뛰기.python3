from itertools import permutations
def solution(n):
    answer = 0
    jumps = [1 for _ in range(n)]
    L = n
    two = 0

    while True:
        permut = list(set(permutations(jumps)))
        answer += len(permut)

        if L <= 1:
            break

        a = jumps.pop(0)
        b = jumps.pop(0)
        if a >= 2 or b >= 2:
            break
        jumps.append(2)
        L -= 1
    return answer % 1234567