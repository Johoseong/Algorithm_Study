def solution(s):
    answer = []
    tuples = s[2:-2].split('},{')

    tuples.sort(key = lambda x:len(x)) # 길이로 오름차순 정렬
    
    for i in range(len(tuples)):
        tuple_set = tuples[i].split(',')
        for j in tuple_set:
            if j not in answer:
                answer.append(j)

    answer = list(map(int, answer))
    return answer